{
  "presets": ["es2015", "react"],
  "plugins": [
    "transform-async-to-generator",
    "transform-object-rest-spread"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  }
}